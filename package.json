{
  "name": "autobet-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run init && next dev --turbopack",
    "build": "npm run init && next build --turbopack",
    "start": "npm run init && next start",
    "lint": "eslint",
    "init": "tsx scripts/app-init.ts",
    "init-db": "tsx scripts/init-db.ts",
    "setup-db": "tsx scripts/setup-db.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm -- migration:generate -d ormconfig.ts",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:run": "npm run typeorm -- migration:run -d ormconfig.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d ormconfig.ts",
    "schema:drop": "npm run typeorm -- schema:drop -d ormconfig.ts",
    "schema:sync": "npm run typeorm -- schema:sync -d ormconfig.ts"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pg": "^8.15.5",
    "axios": "^1.12.2",
    "axios-cookiejar-support": "^6.0.4",
    "bcryptjs": "^3.0.2",
    "dotenv": "^17.2.3",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.546.0",
    "mysql2": "^3.15.3",
    "next": "15.5.6",
    "pg": "^8.16.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "redis": "^5.8.3",
    "tough-cookie": "^5.1.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.19.23",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.6",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typeorm": "^0.3.27",
    "typescript": "^5"
  }
}
